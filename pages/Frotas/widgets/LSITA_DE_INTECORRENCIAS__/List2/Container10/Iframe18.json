  "srcDoc": "<style>\n    * { margin: 0; padding: 0; box-sizing: border-box; }\n    \n    body {\n        font-family: -apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Arial, sans-serif;\n        background: transparent;\n        padding: 0;\n        font-size: 11px;\n    }\n    \n    /* Card de Intercorr√™ncia - 600x200px */\n    .intercorrencia-card {\n        width: 671px;\n        height: 200px;\n        background: white;\n        border-radius: 10px;\n        padding: 12px;\n        box-shadow: 0 3px 12px rgba(239, 68, 68, 0.1);\n        border-left: 5px solid #ef4444;\n        transition: all 0.2s;\n        margin: 0;\n        position: relative;\n        overflow: hidden;\n        display: flex;\n        flex-direction: column;\n    }\n    \n    .intercorrencia-card:hover {\n        transform: translateY(-1px);\n        box-shadow: 0 4px 12px rgba(239, 68, 68, 0.2);\n    }\n    \n    /* Header Compacto */\n    .card-header {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        margin-bottom: 8px;\n        flex-shrink: 0;\n    }\n    \n    .intercorrencia-id {\n        font-size: 16px;\n        font-weight: 800;\n        color: #ef4444;\n        background: #fef2f2;\n        padding: 4px 8px;\n        border-radius: 6px;\n        border: 2px solid #ef4444;\n    }\n    \n    .badge-cidade {\n        background: #1890ff;\n        color: white;\n        padding: 4px 8px;\n        border-radius: 6px;\n        font-size: 14px;\n        font-weight: 700;\n    }\n    \n    /* Layout Principal Compacto */\n    .card-body {\n        display: grid;\n        grid-template-columns: 1fr 120px;\n        gap: 12px;\n        flex: 1;\n        overflow: hidden;\n    }\n    \n    .info-principal {\n        display: flex;\n        flex-direction: column;\n        gap: 4px;\n        overflow: hidden;\n    }\n    \n    .info-linha {\n        display: flex;\n        align-items: center;\n        gap: 6px;\n        font-size: 12px;\n        line-height: 1.3;\n    }\n    \n    .info-label {\n        font-weight: 700;\n        color: #6b7280;\n        min-width: 45px;\n        flex-shrink: 0;\n    }\n    \n    .info-value {\n        font-weight: 600;\n        color: #1f2937;\n        flex: 1;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n    }\n    \n    /* Motoboy Compacto */\n    .motoboy-compact {\n        background: #f0f9ff;\n        border-radius: 4px;\n        padding: 5px 7px;\n        border-left: 3px solid #1890ff;\n        margin: 2px 0;\n        flex-shrink: 0;\n        height: 28px;\n    }\n    \n    .motoboy-nome {\n        font-size: 12px;\n        font-weight: 700;\n        color: #1890ff;\n        line-height: 1.2;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n    }\n    \n    .motoboy-codigo {\n        font-size: 10px;\n        color: #6b7280;\n        line-height: 1.1;\n    }\n    \n    /* Descri√ß√£o Compacta */\n    .descricao-compact {\n        background: #fef2f2;\n        border-radius: 4px;\n        padding: 5px 7px;\n        margin: 2px 0;\n        border-left: 3px solid #ef4444;\n        overflow: hidden;\n        flex-shrink: 0;\n        height: 32px;\n    }\n    \n    .descricao-texto {\n        font-size: 11px;\n        color: #1f2937;\n        line-height: 1.3;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        display: -webkit-box;\n        -webkit-line-clamp: 2;\n        -webkit-box-orient: vertical;\n        line-clamp: 2;\n        max-height: 26px;\n    }\n    \n    /* √Årea da Imagem */\n    .imagem-area {\n        display: flex;\n        flex-direction: column;\n        gap: 4px;\n        width: 120px;\n    }\n    \n    .tempo-mini {\n        background: #fff7ed;\n        border: 2px solid #f59e0b;\n        border-radius: 6px;\n        padding: 6px 4px;\n        text-align: center;\n        height: 35px;\n        flex-shrink: 0;\n    }\n    \n    .tempo-valor {\n        font-size: 11px;\n        font-weight: 800;\n        color: #f59e0b;\n        font-family: 'Courier New', monospace;\n        line-height: 1.1;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n    }\n    \n    .tempo-label {\n        font-size: 8px;\n        color: #92400e;\n        font-weight: 600;\n        text-transform: uppercase;\n        line-height: 1;\n    }\n    \n    .imagem-mini {\n        border-radius: 6px;\n        overflow: hidden;\n        background: #f9fafb;\n        border: 2px solid #e5e7eb;\n        flex: 1;\n        min-height: 100px;\n    }\n    \n    .imagem-mini img {\n        width: 100%;\n        height: 100%;\n        object-fit: cover;\n        cursor: pointer;\n        transition: transform 0.2s;\n    }\n    \n    .imagem-mini img:hover {\n        transform: scale(1.05);\n    }\n    \n    .sem-imagem {\n        display: flex;\n        align-items: center;\n        justify-content: center;\n        height: 100%;\n        color: #9ca3af;\n        font-size: 11px;\n        font-weight: 600;\n        text-align: center;\n    }\n    \n    /* Rodap√© Compacto */\n    .card-footer {\n        display: flex;\n        justify-content: space-between;\n        align-items: center;\n        font-size: 10px;\n        color: #6b7280;\n        border-top: 1px solid #f3f4f6;\n        padding-top: 4px;\n        margin-top: 4px;\n        flex-shrink: 0;\n        height: 18px;\n    }\n    \n    .lancador-info {\n        overflow: hidden;\n        flex: 1;\n    }\n    \n    .lancador-nome {\n        font-weight: 600;\n        overflow: hidden;\n        text-overflow: ellipsis;\n        white-space: nowrap;\n        max-width: 150px;\n        line-height: 1;\n    }\n</style>\n\n<!-- Card Compacto de Intercorr√™ncia - 357x173px -->\n<div class=\"intercorrencia-card\">\n    <!-- Header -->\n    <div class=\"card-header\">\n        <div class=\"intercorrencia-id\"># {{ currentItem.ID_INTERCORRENCIA }}</div>\n        <div class=\"badge-cidade\">{{ currentItem.CIDADE }}</div>\n    </div>\n    \n    <!-- Corpo Principal -->\n    <div class=\"card-body\">\n        <!-- Coluna Principal -->\n        <div class=\"info-principal\">\n            <!-- Info B√°sica -->\n            <div class=\"info-linha\">\n                <div class=\"info-label\">üö¶ Sa√≠da:</div>\n                <div class=\"info-value\">{{ currentItem.SAIDA_ID }}</div>\n            </div>\n            <div class=\"info-linha\">\n                <div class=\"info-label\">üì¶ Pedido:</div>\n                <div class=\"info-value\">{{ currentItem.NUMPED }}</div>\n            </div>\n            <div class=\"info-linha\">\n                <div class=\"info-label\">üè¢ Filial:</div>\n                <div class=\"info-value\">{{ currentItem.ID_FILIAL }}</div>\n            </div>\n            <div class=\"info-linha\">\n                <div class=\"info-label\">üìÖ Data:</div>\n                <div class=\"info-value\">{{ currentItem.DATA_INTERCORRENCIA }}</div>\n            </div>\n            \n            <!-- Motoboy -->\n            <div class=\"motoboy-compact\">\n                <div class=\"motoboy-nome\">üèçÔ∏è {{ currentItem.NOME_MOTOBOY }}</div>\n                <div class=\"motoboy-codigo\">C√≥d: {{ currentItem.COD_MOTOBOY }}</div>\n            </div>\n            \n            <!-- Descri√ß√£o -->\n            <div class=\"descricao-compact\">\n                <div class=\"descricao-texto\">{{ currentItem.DESCRICAO }}</div>\n            </div>\n        </div>\n        \n        <!-- Coluna da Direita -->\n        <div class=\"imagem-area\">\n            <!-- Tempo -->\n            <div class=\"tempo-mini\">\n                <div class=\"tempo-valor\">{{ currentItem.TEMPO_INTERCORRENCIA }}</div>\n                <div class=\"tempo-label\">TEMPO</div>\n            </div>\n            \n            <!-- Imagem -->\n            <div class=\"imagem-mini\">\n                <img src=\"{{ currentItem.URLS_FOTOS }}\" \n                     alt=\"Foto #{{ currentItem.ID_INTERCORRENCIA }}\" \n                     onclick=\"abrirImagem('{{ currentItem.URLS_FOTOS }}', '{{ currentItem.ID_INTERCORRENCIA }}')\"\n                     onerror=\"this.style.display='none'; this.nextElementSibling.style.display='flex';\"\n                     title=\"üì∑ Clique para ampliar\">\n                <div class=\"sem-imagem\" style=\"display: none;\">üì∑ Sem foto</div>\n            </div>\n        </div>\n    </div>\n    \n    <!-- Rodap√© -->\n    <div class=\"card-footer\">\n        <div class=\"lancador-info\">\n            <div class=\"lancador-nome\">üìù {{ currentItem.NOME_LANCAMENTO }}</div>\n        </div>\n        <div style=\"font-weight: 700; color: #ef4444; font-size: 8px;\">\n            ID: {{ currentItem.ID_INTERCORRENCIA }}\n        </div>\n    </div>\n</div>\n\n<script>\n// Fun√ß√£o para abrir imagem em nova aba (compat√≠vel com Appsmith)\nfunction abrirImagem(urlImagem, idIntercorrencia) {\n    if (urlImagem && urlImagem.trim() && urlImagem !== 'undefined' && urlImagem !== '') {\n        try {\n            window.open(urlImagem, '_blank');\n            console.log('üì∑ Imagem aberta para intercorr√™ncia #' + idIntercorrencia);\n        } catch (error) {\n            console.error('Erro ao abrir imagem:', error);\n            alert('Erro ao abrir imagem');\n        }\n    } else {\n        alert('Sem imagem dispon√≠vel para esta intercorr√™ncia');\n    }\n}\n</script>\n\n{{''}}\n",
