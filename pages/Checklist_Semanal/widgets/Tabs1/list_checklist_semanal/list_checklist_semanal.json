  "listData": "{{\n  (() => {\n    const checklist = verificar_checklist_semanal.data || [];\n\n    return checklist.map(item => {\n\n      // Data formatada (semanal vem YYYY-MM-DD)\n      const dataFormatada = (() => {\n        const normalizado = String(item.data_checklist || \"\").replace(/\\.\\d+$/, \"\");\n        const m = moment(\n          normalizado,\n          [\"YYYY-MM-DD\", \"YYYY-MM-DD HH:mm:ss\", moment.ISO_8601],\n          true\n        );\n        return m.isValid()\n          ? m.format(\"DD/MM/YYYY\")\n          : normalizado;\n      })();\n\n      return {\n        id_checklist: item.id_checklist,\n        placa: item.placa,\n        data_checklist: dataFormatada,\n        usuario_execucao: item.usuario_execucao || \"â€”\",\n        filial_id: item.filial_id,\n        qtd_nc: Number(item.qtd_nc) || 0,\n        observacoes: item.observacoes || \"\",\n        resultado_final: item.resultado_final\n      };\n    });\n  })()\n}}\n",
